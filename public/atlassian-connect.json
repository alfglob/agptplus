{
  "key": "alexgpt-plus",
  "name": "AlexGPT+",
  "description": "Integration of AlexGPT+ for JIRA",
  "vendor": {
    "name": "Globant",
    "url": "globant.com"
  },
  "baseUrl": "https://jira-example.onrender.com",
  "links": {
    "config": "https://jira-example.onrender.com"
  },
  "authentication": {
    "type": "none"
  },
  "scopes": [
    "READ",
    "WRITE",
    "ACT_AS_USER"
  ],
