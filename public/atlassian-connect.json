    "generalPages": [
      {
        "key": "prompt",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "AlexGPT+"
        },
        "url": "/configure-your-prompt",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ]
      }
    ]
  }
}